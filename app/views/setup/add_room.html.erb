<h1>Add room to <%= @tournament.name %></h1>

<% form_for :room do |f| %>
  <p>
    <label for="room[name]">Room name/number: </label>
      <%= f.text_field :name %>
  </p>
  
  <p>
    <label for="room[staffer]">Room staffer(s): </label>
      <%= f.text_field :staffer %>
  </p>
  
  
  <%= f.submit %>
<% end %>